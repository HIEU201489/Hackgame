      <key>PayloadType</key>
      <string>com.apple.wifi.managed</string>
      <key>PayloadVersion</key>
      <integer>1</integer>
      <key>PayloadIdentifier</key>
      <string>com.hieu.wifi</string>
      <key>PayloadUUID</key>
      <string>2A1B2C3D-4E5F-6789-AAAA-BBBBBBBBBBBB</string>
      <key>PayloadDisplayName</key>
      <string>Wi-Fi Game</string>
      <key>SSID_STR</key>
      <string>Game_WIFI</string>
      <key>AutoJoin</key>
      <true/>
      <key>EncryptionType</key>
      <string>WPA</string>
      <key>Password</key>
      <string>12345678</string>
    </dict>

    <!-- 2. Dữ liệu không xác định (dummy) -->
